integrator               = md
tcoupl                   = berendsen

dt                       = 0.002         ; ps 
nsteps_eq                = 100000       ; 0.2 ns
nsteps_pr                = 200000       ; 0.4 ns

nstxout                  = 200            ; 1000 snapshots
nstlog                   = 500
nstenergy                = 500

vdwtype                  = User
DispCorr                 = EnerPres
vdw-modifier             = None
rvdw                     = 1.4
rvdw-switch              = 0.0
coulombtype              = User
rcoulomb                 = 1.4
rlist                    = 1.4   
cutoff-scheme            = group

constraints              = all-bonds
constraint-algorithm     = LINCS
lincs-order              = 8

gen-vel_eq               = yes
gen-vel_pr               = no
continuation_eq          = no
continuation_pr          = yes
