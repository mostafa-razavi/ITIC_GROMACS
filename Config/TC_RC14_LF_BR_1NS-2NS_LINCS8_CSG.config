integrator               = md
tcoupl                   = berendsen

dt                       = 0.002         ; ps 
nsteps_eq                = 500000       ; 1 ns
nsteps_pr                = 1000000       ; 2 ns

nstxout                  = 1000            ; 1000 snapshots
nstlog                   = 500
nstenergy                = 500

vdwtype                  = Cut-off
DispCorr                 = EnerPres
vdw-modifier             = None
rvdw                     = 1.4
rvdw-switch              = 0.0
coulombtype              = Cut-off
rcoulomb                 = 1.4
rlist                    = 1.4   
cutoff-scheme            = group

constraints              = all-bonds
constraint-algorithm     = LINCS
lincs-order              = 8

gen-vel_eq               = yes
gen-vel_pr               = no
continuation_eq          = no
continuation_pr          = yes
